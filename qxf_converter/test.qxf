<?xml version="1.0" encoding="UTF-8"?>
<FixtureDefinition>
  <Creator>
    <Name>Gemini</Name>
    <Version>1.0</Version>
    <Author>Code Assist</Author>
  </Creator>
  <Manufacturer>Generic</Manufacturer>
  <Model>LED Par 64</Model>
  <Type>Color Changer</Type>
  <!-- Channels -->
  <Channel Name="Red">
    <Group Byte="0">Intensity</Group>
    <Colour>Red</Colour>
    <Capability Min="0" Max="255">Red 0-100%</Capability>
  </Channel>
  <Channel Name="Green">
    <Group Byte="0">Intensity</Group>
    <Colour>Green</Colour>
    <Capability Min="0" Max="255">Green 0-100%</Capability>
  </Channel>
  <Channel Name="Blue">
    <Group Byte="0">Intensity</Group>
    <Colour>Blue</Colour>
    <Capability Min="0" Max="255">Blue 0-100%</Capability>
  </Channel>
  <Channel Name="Strobe">
    <Group Byte="0">Speed</Group>
    <Capability Min="0" Max="10">No function</Capability>
    <Capability Min="11" Max="255">Strobe slow to fast</Capability>
  </Channel>
  <!-- Modes -->
  <Mode Name="3-Channel">
    <Channel Number="0">Red</Channel>
    <Channel Number="1">Green</Channel>
    <Channel Number="2">Blue</Channel>
  </Mode>
  <Mode Name="4-Channel">
    <Channel Number="0">Red</Channel>
    <Channel Number="1">Green</Channel>
    <Channel Number="2">Blue</Channel>
    <Channel Number="3">Strobe</Channel>
  </Mode>
  <!-- Physical for the whole fixture -->
  <Physical>
    <Bulb Type="LED" Lumens="1500" ColourTemperature="0"/>
    <Dimensions Weight="2.5" Width="220" Height="220" Depth="240"/>
    <Lens Name="PC" DegreesMin="25" DegreesMax="25"/>
    <Focus Type="Fixed" PanMax="0" TiltMax="0"/>
    <Technical PowerConsumption="30" DmxConnector="3-pin"/>
  </Physical>
</FixtureDefinition>
